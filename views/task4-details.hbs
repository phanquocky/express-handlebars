
    <!--contain-->
    <div class="container">
        <h1 class="text-center">{{zodiac.name}} Characteristics</h1>
        <div class="container-fluid well span6">
            <div class="row">
                <div class="col-md-4">
                    <img src="images{{zodiac.imagePath}}" class="img-circle">
                </div>
                <div class="col-md-8">
                    <h3>{{zodiac.name}}</h3>
                    <h4>Strengths: <small>{{zodiac.strengths}}</small></h4>
                    <h4>Weaknesses: <small>{{zodiac.weaknesses}}</small>
                    </h4>
                    <h4>Likes: <small>{{zodiac.likes}}</small></h4>
                    <h4>Dislikes: <small>{{zodiac.dislikes}}</small></h4>
                    <h4><small>{{zodiac.description}}</small></h4>
                </div>
            </div>
        </div>
    </div>

    <!--/contain-->
    